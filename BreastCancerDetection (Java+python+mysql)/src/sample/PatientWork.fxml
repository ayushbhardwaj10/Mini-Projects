<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="680.0" prefWidth="1300.0" styleClass="anchor13" stylesheets="@AdminWork.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PatientWork">
   <children>
      <ImageView fitHeight="161.0" fitWidth="202.0" layoutX="1029.0" layoutY="79.0" onMouseClicked="#chatbotOpen" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/customerSupport.png" />
         </image></ImageView>
      <Label layoutX="1056.0" layoutY="240.0" prefHeight="35.0" prefWidth="125.0" text="Talk to us" textFill="#f5eaea">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <effect>
            <DropShadow blurType="TWO_PASS_BOX" color="#1e0310" height="32.86" radius="12.965" spread="1.0" />
         </effect>
      </Label>
      <Label layoutX="29.0" layoutY="333.0" prefHeight="35.0" prefWidth="243.0" text="Enter your Patient id">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="id" layoutX="301.0" layoutY="322.0" prefHeight="56.0" prefWidth="286.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextField>
      <Label fx:id="result" alignment="CENTER" layoutX="301.0" layoutY="578.0" prefHeight="56.0" prefWidth="236.0" style="-fx-background-color: #B8860B;" text="Result...." textFill="#eeeeee">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button layoutX="301.0" layoutY="439.0" mnemonicParsing="false" onAction="#checkCancerLevel" prefHeight="56.0" prefWidth="161.0" style="-fx-background-radius: 15; -fx-background-color: #FFD700;" text="Check">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="92.0" fitWidth="122.0" layoutX="1144.0" layoutY="560.0" onMouseClicked="#returnHome" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resources/back.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="455.0" layoutY="60.0" prefHeight="46.0" prefWidth="352.0" text="Welcome Patient">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
         <effect>
            <DropShadow blurType="TWO_PASS_BOX" color="#cda331" height="26.93" radius="15.93" spread="1.0" width="38.79" />
         </effect>
      </Label>
   </children>
</AnchorPane>
